<?xml version="1.0" encoding="utf-8"?>
<!-- Suggestion Card Background -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#2A3441"/>
            <corners android:radius="10dp"/>
            <stroke android:width="1.5dp" android:color="#00E5C4"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="#0D2E2A"/>
            <corners android:radius="10dp"/>
            <stroke android:width="1.5dp" android:color="#00E5C4"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#1E2833"/>
            <corners android:radius="10dp"/>
            <stroke android:width="1dp" android:color="#2A3441"/>
        </shape>
    </item>
</selector>
